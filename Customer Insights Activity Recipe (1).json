            ]
        },
        "FORMULA3": {
            "action": "formula",
            "parameters": {
                "expressionType": "SQL",
                "fields": [
                    {
                        "defaultValue": "",
                        "formulaExpression": "case \n\twhen IsOverdue in ('true')\n\tthen abs(datediff(now(),to_date(ActivityDate, 'yyyy-MM-dd')))\n\telse\n\t0\nend",
                        "label": "DaysOverdue",
                        "name": "DaysOverdue",
                        "precision": 10,
                        "scale": 0,
                        "type": "NUMBER"
                    }
                ]
            },
            "sources": [
                "FORMULA2"
            ]
