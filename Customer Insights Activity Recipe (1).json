        "FORMULA2": {
            "action": "formula",
            "parameters": {
                "expressionType": "SQL",
                "fields": [
                    {
                        "defaultValue": "",
                        "formulaExpression": "case \n\twhen \"Opp.IsClosed\" in ('false') and datediff(now(), to_date(ActivityDate, 'yyyy-MM-dd')) > 0\n\tthen 'true'\n\telse\n\t'false'\nend",
                        "label": "IsOverdue",
                        "name": "IsOverdue",
                        "precision": 255,
                        "type": "TEXT"
                    }
                ]
            },
            "sources": [
